,e}recalcTotalFila(t){this.numberService.recalcTotalFila(t),this.totalizar()}checkPagosPrevios(){this.formcab.trn_codigo>0&&this.numberService.checkPagosPrevios(this.datosDocPlan.doc.pagos,this.totales,this.formaspago)}totalizar(){this.initTotales(),this.totales=this.numberService.totalizar(this.detalles),this.formaspago&&this.formaspago.length>0&&(this.formaspago[0].dt_valor=0),this.formaspago&&this.formaspago.length>1&&(this.formaspago[1].dt_valor=this.totales.total)}creaPlanTratamiento(){if(0===this.formplan.pnt_nombre.trim().length)this.swalService.fireToastError("Debe ingresar el nombre del plan");else if(0===this.detalles.length)this.swalService.fireToastError("Debe agregar items al plan de tratamiento");else{const t=this.formcab.trn_codigo>0;this.swalService.fireDialog(`\xbfConfirma la ${t?"actualizaci\xf3n":"creaci\xf3n"} de este plan de tratamiento?`).then(e=>{if(e.value){this.formcab.trn_docpen="T";const e={formplan:this.formplan,form_cab:this.formcab,form_persona:{per_id:this.codpaciente},detalles:this.detalles,pagos:this.formaspago,totales:this.totales};this.loadinUIServ.publishBlockMessage(),this.planService.crear(e).subscribe(e=>{200===e.status&&(this.swalService.fireToastSuccess(e.msg),this.showForm=!1,this.loadPlanes(),t&&this.loadDatosPlan(this.plansel.pnt_id))})}})}}quitarItem(t){this.swalService.fireDialog("\xbfSeguro que desea quitar este item?").then(e=>{e.value&&(this.arrayService.removeElement(this.detalles,t),this.totalizar())})}onMedicoChange(t){}selectServicio(t){const e=this.arrayService.getFirstResult(this.detalles,e=>e.art_codigo===t.ic_id);e?(e.dt_cant+=1,this.recalcTotalFila(e)):this.detalles.push(this.getNewEmptyRow(t)),this.totalizar()}cancelarCreaPlan(){this.showForm=!1}loadDatosPlan(t){this.loadinUIServ.publishBlockMessage(),this.planService.getDetallesPlan(t).subscribe(t=>{200===t.status&&(this.datosDocPlan.plan=t.plan,this.datosDocPlan.doc=t.doc)})}showDetallesPlan(t){this.plansel=t,this.loadDatosPlan(this.plansel.pnt_id)}cerrarV